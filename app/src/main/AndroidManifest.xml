<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nagaraj.f_evoting">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".voting"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".signUp"
            android:label="@string/signUp">
        </activity>
        <activity android:name=".signIn"
            android:label="@string/signIn">
        </activity>
        <activity android:name=".category"
            android:label="@string/category">
        </activity>
        <activity android:name=".vp"
            android:label="@string/vp">
        </activity>
        <activity android:name=".gs"
            android:label="@string/gs">
        </activity>
        <activity android:name=".results"
            android:label="@string/results">
        </activity>
    </application>
</manifest>
